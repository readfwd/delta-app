<?xml version='1.0' encoding='utf-8'?>
<widget id="com.readfwd.deltaapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Danube Delta</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <platform name="ios">
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="res/icons/ios/AppIcon60x60.png" width="60" height="60" />
        <icon src="res/icons/ios/AppIcon60x60@2x.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/icons/ios/AppIcon76x76.png" width="76" height="76" />
        <icon src="res/icons/ios/AppIcon76x76@2x.png" width="152" height="152" />
        <!-- iOS 6.1 -->
        <!-- Spotlight Icon -->
        <icon src="res/icons/ios/AppIcon40x40.png" width="40" height="40" />
        <icon src="res/icons/ios/AppIcon40x40@2x.png" width="80" height="80" />
        <!-- iPhone / iPod Touch -->
        <icon src="res/icons/ios/AppIcon57x57.png" width="57" height="57" />
        <icon src="res/icons/ios/AppIcon57x57@2x.png" width="114" height="114" />
        <!-- iPad -->
        <icon src="res/icons/ios/AppIcon72x72.png" width="72" height="72" />
        <icon src="res/icons/ios/AppIcon72x72@2x.png" width="144" height="144" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/icons/ios/AppIcon29x29.png" width="29" height="29" />
        <icon src="res/icons/ios/AppIcon29x29@2x.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/icons/ios/AppIcon50x50.png" width="50" height="50" />
        <icon src="res/icons/ios/AppIcon50x50@2x.png" width="100" height="100" />
    </platform>
    <platform name="android">
        <icon src="res/icons/android/ldpi.png" density="ldpi" />
        <icon src="res/icons/android/mdpi.png" density="mdpi" />
        <icon src="res/icons/android/hdpi.png" density="hdpi" />
        <icon src="res/icons/android/xhdpi.png" density="xhdpi" />
    </platform>
</widget>
